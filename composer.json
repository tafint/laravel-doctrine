{
	"name": "atrauzzi/laravel-doctrine",
	"description": "An ORM for a Framework for Web Artisans",
	"keywords": [
		"doctrine",
		"laravel",
		"orm"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Alexander Trauzzi",
			"email": "atrauzzi@gmail.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"laravel/framework": ">=5.0.0 <5.2",
		"doctrine/orm": ">=2.4.0",
		"doctrine/migrations": "~1.2.0",
		"beberlei/DoctrineExtensions": "1.0.*@dev"
	},
    "require-dev":{
        "phpspec/phpspec": "2.2.*"
    },
	"autoload": {
		"psr-4": {
			"Atrauzzi\\LaravelDoctrine\\": "src/"
		}
	},
	"bin": [
		"bin/doctrine-laravel",
		"bin/doctrine-laravel.php"
	]
}
